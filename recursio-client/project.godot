[application]

config/name="recursio"
run/main_scene="res://World.tscn"
boot_splash/image="res://lizrad_logo.png"
config/icon="res://icon.png"

[autoload]

Constants="*res://Shared/Constants.gd"
Logger="*res://addons/recursio-loggerplugin/Logger.gd"
LoggerPluginSingleton="*res://addons/recursio-loggerplugin/LoggerPluginSingleton.gd"
Server="*res://Global/Server.gd"
InputManager="*res://Global/InputManager.gd"
GlobalActionManager="*res://Shared/Actions/ActionManager.gd"

[debug]

multirun/number_of_windows=2
multirun/window_distance=1024
multirun/add_custom_args=false
multirun/first_window_args=""
multirun/other_window_args=""
multirun/window_distance_x=0
multirun/window_distance_y=0

[display]

window/vsync/use_vsync=false
window/stretch/mode="2d"

[editor_plugins]

enabled=PoolStringArray( "res://addons/multirun/plugin.cfg", "res://addons/project_editor_settings/plugin.cfg", "res://addons/recursio-loggerplugin/plugin.cfg" )

[global]

layer=false

